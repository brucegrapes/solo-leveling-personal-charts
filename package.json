{
  "name": "solo-leveling-personal-charts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "migrate": "npx ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/migrate-activities.ts",
    "migrate:prod": "NODE_ENV=production npx ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/migrate-activities.ts",
    "migrate:local": "MONGODB_URI=mongodb://localhost:27018/solo-leveling-local npx ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/migrate-activities.ts",
    "db:migrate:prod": "MONGODB_URI=mongodb://mongodb:27017/solo-leveling migrate-mongo up",
    "db:migrate:down": "migrate-mongo down",
    "db:migrate:status": "migrate-mongo status",
    "db:migrate:create": "migrate-mongo create",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:prod": "bash generate-ssl.sh && docker compose up -d --build",
    "docker:logs": "docker compose logs -f",
    "docker:restart": "docker compose restart",
    "docker:clean": "docker compose down -v && rm -rf .next node_modules/.cache",
    "docker:local:up": "docker compose -f docker-compose.local.yml up -d",
    "docker:local:down": "docker compose -f docker-compose.local.yml down",
    "docker:local:build": "docker compose -f docker-compose.local.yml up -d --build",
    "docker:local:logs": "docker compose -f docker-compose.local.yml logs -f",
    "docker:local:restart": "docker compose -f docker-compose.local.yml restart",
    "docker:local:clean": "docker compose -f docker-compose.local.yml down -v",
    "docker:local:db": "docker compose -f docker-compose.local.yml up -d mongodb-local"
  },
  "dependencies": {
    "@sentry/browser": "^10.34.0",
    "bcryptjs": "^2.4.3",
    "date-fns": "^3.3.1",
    "jose": "^5.10.0",
    "migrate-mongo": "^14.0.7",
    "minio": "^8.0.6",
    "mongoose": "^9.1.1",
    "next": "^15.1.0",
    "next-auth": "^5.0.0-beta.30",
    "node-cron": "^4.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.12.7",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.10.2",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@types/web-push": "^3.6.4",
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.6.1",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.0",
    "postcss": "^8.4.49",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  }
}